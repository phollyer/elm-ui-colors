module Color.Lavender exposing (lavender, lavenderblush)

{-| The Lavender range of Colors.


# HTML/CSS color names

@docs lavender, lavenderblush

-}

import Element exposing (Color, rgb255)


{-| rgb(230, 230, 250)
#E6E6FA
-}
lavender : Color
lavender =
    rgb255 230 230 250


{-| rgb(255, 240, 245)
#FFF0F5
-}
lavenderblush : Color
lavenderblush =
    rgb255 255 240 245
